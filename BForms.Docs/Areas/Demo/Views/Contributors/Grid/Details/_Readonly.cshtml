@using BForms.Docs.Resources
@using BootstrapForms.Html
@model BForms.Docs.Areas.Demo.Models.ContributorDetailsModel

<div class="js-readonly">
    @if (!string.IsNullOrEmpty(Model.Url))
    {
        <dl>
            <dt>url &nbsp;</dt>
            <dd><a href="@Model.Url" target="_blank" title="@Model.Url">@Model.Url</a></dd>
        </dl>
    }
    @if (!string.IsNullOrEmpty(Model.Contributions))
    {
        <dl>
            <dt>contributions &nbsp;</dt>
            <dd>@Model.Contributions</dd>
        </dl>
    }
</div>
